<h1>Listing studenttests</h1>

<table>
  <tr>
    <th>Test</th>
    <th>Student</th>
    <th>Start</th>
    <th>End</th>
    <th>Points</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @studenttests.each do |studenttest| %>
  <tr>
    <td><%= studenttest.test_id %></td>
    <td><%= studenttest.student_id %></td>
    <td><%= studenttest.start %></td>
    <td><%= studenttest.end %></td>
    <td><%= studenttest.points %></td>
    <td><%= link_to 'Show', studenttest %></td>
    <td><%= link_to 'Edit', edit_studenttest_path(studenttest) %></td>
    <td><%= link_to 'Destroy', studenttest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Studenttest', new_studenttest_path %>
