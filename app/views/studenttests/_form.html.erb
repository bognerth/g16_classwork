<%= simple_form_for(@studenttest) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :test_id %>
    <%= f.input :student_id %>
    <%= f.input :start %>
    <%= f.input :end %>
    <%= f.input :points %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
