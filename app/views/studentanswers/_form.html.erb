<%= simple_form_for(@studentanswer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :question_id %>
    <%= f.input :studenttest_id %>
    <%= f.input :points %>
    <%= f.input :title %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
